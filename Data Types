<html>
    <body>
        <h1>Data Types In JS</h1>
        <script>
            var a = 7;
            var b = 9;
            var c = 9;
            var d = null;
            var e; //undefined
            document.write("The value of a is = " + a);
            document.write("<br>");

            document.write("The value of b is = " + b);
            document.write("<br>");

            document.write("The value of c is = " + c);
            document.write("<br>");
            
            document.write("The value of d is = " + d);
            document.write("<br>");

            document.write("The value of e is = " + e);
            document.write("<br>");

            document.write("a and b are equal = " + (a == b));
            document.write("<br>");

            document.write("b and c are equal = " + (c == b));
            document.write("<br>");

        </script>
    </body>
</html>