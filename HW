<html>
    <body>
        <h1>Number Series</h1>
        <p id="rl"></p>

        <script>
            var i = 1;
            var text = "";

            while (i < 101) {
                text += "<br>The number is: " + i;
                i++;
            }

            document.getElementById("rl").innerHTML = text;
        </script>
    </body>
</html>